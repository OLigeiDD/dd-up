{
  "packages": [
    "packages/*"
  ],
  "command": {
    "bootstrap": {
      "hoist": true,
      "noCi": true,
      "npmClientArgs": [
        "--no-package-lock"
      ]
    },
    "publish": {
      "ignoreChanges": [
        "*.md"
      ],
      "verifyAccess": false,
      "registry": "https://registry.npmjs.org/"
    },
    "version": {
      "conventionalCommits": true
    }
  },
  "npmClient": "npm",
  "version": "0.0.6"
}
